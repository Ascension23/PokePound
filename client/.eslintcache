 from '../utils/auth';\n\nconst Place = () => {\n  const [formState, setFormState] = useState({\n    name: '',\n    description: '',\n  });\n\n  const [addAdoption, { error, data }] = useMutation(ADD_ADOPTION);\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n    setFormState({\n      ...formState,\n      [name]: value,\n    });\n  };\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n    console.log(formState);\n\n    try {\n      const { data } = await addAdoption({\n        variables: { ...formState },\n      });\n\n      // Auth.getToken(data.addAdoption.token);\n      \n    } catch (e) {\n      console.error(e);\n    }\n  };\n\n  return (\n    <main className=\"flex-row justify-center mb-4\">\n      <div className=\"col-10 col-lg-8\">\n        <div className=\"card\">\n          <h4 className=\"card-header bg-dark text-light p-2\">Place Pokemon up for Adoption</h4>\n          <div className=\"card-body\">\n            {data ? (\n              <p>\n                Success! You may now head{' '}\n                <Link to=\"/\">back to the homepage.</Link>\n              </p>\n            ) : (\n              <Form onSubmit={handleFormSubmit}>\n                <Form.Group className=\"mb-3\" controlId=\"formName\">\n                  <Form.Label>name</Form.Label>\n                  <Form.Control type=\"text\" name=\"name\" placeholder=\"name . . .\" value={formState.name} onChange={handleChange}/>\n                </Form.Group>\n                <Form.Group className=\"mb-3\" controlId=\"formBasicPassword\">\n                  <Form.Label>description</Form.Label>\n                  <Form.Control type=\"text\" name=\"description\" placeholder=\"description\" value={formState.description} onChange={handleChange}/>\n                </Form.Group>\n\n                <Button variant=\"primary\" type=\"submit\">Submit</Button>\n              </Form>\n            )}\n\n            {error && (\n              <div className=\"my-3 p-3 bg-danger text-white\">\n                {error.message}\n              </div>\n            )}\n          </div>\n        </div>\n      </div>\n    </main>\n  );\n};\n\nexport default Place;\n","/Users/scottlawan/Documents/Boot_Camp_2021/Project_03/test_03/client/src/pages/Profile.js",[],"/Users/scottlawan/Documents/Boot_Camp_2021/Project_03/test_03/client/src/components/Header/index.js",["321","322"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport { Nav, Button } from 'react-bootstrap';\nimport Auth from '../../utils/auth';\n\nconst Header = () => {\n  const logout = (event) => {\n    event.preventDefault();\n    Auth.logout();\n  };\n  return (\n\n    <Nav className=\"justify-content-end\" activeKey=\"/home\">\n      <Nav.Item>\n        <Nav.Link href=\"/\">Home</Nav.Link>\n      </Nav.Item>\n      <Nav.Item>\n        <Nav.Link href=\"/place\">Place Pokemon up for Adoption</Nav.Link>\n      </Nav.Item>\n\n        {Auth.loggedIn() ? (\n          <>\n            <Nav.Item>\n              <Nav.Link href=\"/me\">{Auth.getProfile().data.username}'s profile</Nav.Link>\n            </Nav.Item>\n            <Nav.Item>\n              <Nav.Link onClick={logout}>Log Out</Nav.Link>\n            </Nav.Item>\n            {/* <Button variant=\"secondary\" onClick={logout}>\n              Logout\n            </Button> */}\n          </>\n        ) : (\n          <>\n            <Nav.Item>\n              <Nav.Link href=\"/signup\">Sign Up</Nav.Link>\n            </Nav.Item>\n            <Nav.Item>\n              <Nav.Link href=\"login\">Log In</Nav.Link>\n            </Nav.Item>\n          </>\n        )}\n    </Nav>\n  );\n};\n\nexport default Header;\n","/Users/scottlawan/Documents/Boot_Camp_2021/Project_03/test_03/client/src/components/Footer/index.js",["323","324","325","326"],"/Users/scottlawan/Documents/Boot_Camp_2021/Project_03/test_03/client/src/utils/mutations.js",[],"/Users/scottlawan/Documents/Boot_Camp_2021/Project_03/